{
  "manifest_version": 3,
  "name": "SokkerIQ",
  "version": "1.2.4",
  "description": "Displays sumskill information in Sokker Manager online game",
  "icons": {
    "16": "icons/SokkerIQ16.png",
    "48": "icons/SokkerIQ48.png",
    "128": "icons/SokkerIQ128.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "sokker-addon-public@example.com",
      "data_collection_permissions": {
        "required": [
          "none"
        ]
      }
    }
  },
  "action": {
    "default_popup": "popup/popup.html"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.sokker.org/*"
      ],
      "css": [
        "styles.css"
      ],
      "js": [
        "initSettings.js",
        "sokkerSumskill.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "host_permissions": [
    "*://*.sokker.org/*"
  ]
}